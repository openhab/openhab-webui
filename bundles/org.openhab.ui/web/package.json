{
  "name": "openhab",
  "version": "4.2.0",
  "type": "module",
  "description": "openHAB",
  "repository": "https://github.com/openhab/openhab-webui",
  "license": "EPL-2.0",
  "framework7": {
    "type": [
      "web",
      "pwa"
    ],
    "name": "openHAB",
    "pkg": "org.openhab.ui",
    "platforms": [
      "ios",
      "android"
    ],
    "framework": "vue",
    "template": "split-view",
    "cssPreProcessor": "stylus",
    "customColor": true,
    "color": "ff7700",
    "bundler": "vite"
  },
  "scripts": {
    "generate-build-info": "node build/generate-build-info.mjs",
    "generate-widget-component-ts": "node build/generate-widget-component-ts.mjs",
    "build:mvn": "cross-env NODE_ENV=production cross-env MAVEN=true node ./build/build.mjs",
    "build:grammar": "node build/compile-grammar.mjs",
    "predev": "npm run generate-build-info",
    "dev": "vite",
    "prestart": "npm run generate-build-info",
    "start": "vite",
    "lint": "npx eslint src",
    "lint:fix": "npx eslint --fix src",
    "typescript:check": "vue-tsc -b",
    "dependency-analyzer": "npx depcruise src",
    "prebuild": "npm run generate-build-info && npm run generate-widget-component-ts && npm run build:grammar",
    "build": "cross-env NODE_ENV=production vite build",
    "webpack-analyzer-report-stats": "npm run generate-build-info && cross-env NODE_ENV=production cross-env STATS=true vite build",
    "preview": "vite preview",
    "format": "prettier --write src",
    "OLD:predev:blockly": "cross-env NODE_ENV=development npm run generate-build-info",
    "OLD:dev:blockly": "cross-env SOURCE_MAPS=1 NODE_ENV=development webpack-dev-server --config ./build/webpack.config.js",
    "OLD:build:prod": "cross-env NODE_ENV=production node ./build/build.js",
    "test:unit": "npm run generate-build-info && vitest run",
    "test:unit:watch": "npm run generate-build-info && vitest - watch",
    "postinstall": "patch-package"
  },
  "browserslist": [
    "Android >= 5",
    "IOS >= 9.3",
    "Edge >= 15",
    "Safari >= 9.1",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
