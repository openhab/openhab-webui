    "vue-knob-control": "^1.6.0",
    "vue-magic-grid": "0.0.4",
    "vue-qrcode": "^0.3.5",
    "vue2-leaflet": "^2.5.2",
    "vuetrend": "^0.3.4",
    "vuex": "^3.5.1",
    "yaml": "^1.10.0"
  },
