      type: application/javascript;version=ECMAScript-2021
      script: >
        var runtime = require('@runtime');

        var itemRegistry = runtime.itemRegistry;

        var events = runtime.events;


        var qty;



        console.info(('Units of Measurment with ' + String(items.getItem('Temperature').state)));

        if (Quantity(items.getItem('Temperature').state).largerThan(Quantity('15 °C'))) {
          console.info(('It is warm enough in the living room at ' + String(items.getItem('Temperature').state)));
          console.info(('though it would be nices if it was 5 degrees warmer = ' + String(Quantity(items.getItem('Temperature').state).add(Quantity('5 °C')))));
          console.info(('15 degress Celsius are ' + String(Quantity('15 °C').toUnit('°F'))));
        }

        qty = Quantity('10 W');

        console.info(('Quantity=' + String(qty)));

        console.info(('Quantity in kW=' + String(qty.toUnit('kW'))));

        console.warn('Arithmetic operations');

        console.info((qty.add(Quantity('10 kW'))));

        console.info((qty.subtract(Quantity('10 kW'))));

        console.info((qty.multiply(Quantity('10 W'))));

        console.info((qty.multiply(Quantity('10'))));

        console.info((qty.divide(Quantity('10 W'))));

        console.info((qty.divide(Quantity('10'))));

        console.warn('Comparions');

        console.info((qty.equal(Quantity('10 W'))));

        console.info((qty.smallerThan(Quantity('1 kW'))));

        console.info((qty.smallerThanOrEqual(Quantity('10 W'))));

        console.info((qty.largerThan(Quantity('1000 mW'))));

        console.info((qty.largerThanOrEqual(Quantity('10000 mW'))));
