      type: application/javascript;version=ECMAScript-2021
      script: >
        var runtime = require('@runtime');

        var itemRegistry = runtime.itemRegistry;

        var events = runtime.events;


        var namespace, i, configName, fieldName, item;


        function _itemName(item) {
          return (typeof item === "string") ? item : item.name;
        }


        function getItemMetaConfigValue(itemName, namespace, prop) {
          if (items.metadata.getMetadata(_itemName(itemName), namespace)===null) {
             return 'undefined';
          }
          let props = prop.split('.');
          let value = items.metadata.getMetadata(_itemName(itemName), namespace).configuration;
          props.forEach (property => {
            value = value[property];
          });
          return value;
        }


        var itemMedatata;



        namespace = 'namespaceTest';

        configName = 'one.two.three';

        console.info('Testing metadata hierarchies; if vars work, then normal strings work as well, also test bad property access');

        if ((getItemMetaConfigValue(_itemName('teststate'), namespace, configName)) == '756') {
          console.info('one.two.three == 756 => correct');
        } else {
          console.warn('one.two.three == 756 => incorrect');
        }

        configName = 'oone.two';

        if ((getItemMetaConfigValue(_itemName('teststate'), namespace, configName)) == '456') {
          console.info('oone.two == 456 => correct');
        } else {
          console.warn('oone.two != 456 => incorrect');
        }

        configName = 'ooone';

        if ((getItemMetaConfigValue(_itemName('teststate'), namespace, configName)) == '123') {
          console.info('ooone == 123 => correct');
        } else {
          console.warn('ooone != 123 => incorrect');
        }

        configName = 'one';

        console.info((getItemMetaConfigValue(_itemName('teststate'), namespace, configName)));

        if (String(getItemMetaConfigValue(_itemName('teststate'), namespace, configName)) + '' == '{two={three=756}}') {
          console.info('one == {two={three=756}} => correct');
        } else {
          console.warn('one != {two={three=756}} => incorrect');
        }

        configName = 'none';

        console.info((getItemMetaConfigValue(_itemName('teststate'), namespace, configName)));

        if (String(getItemMetaConfigValue(_itemName('teststate'), namespace, configName)) + '' == 'undefined') {
          console.info('none == undefined => correct');
        } else {
          console.warn('none != undefined => incorrect');
        }

        if (((items.metadata.getMetadata(_itemName('teststate'), namespace)!=null) ? (items.metadata.getMetadata(_itemName('teststate'), namespace).value) : 'undefined') == 'Stefan') {
          console.info('value (via item name) == Stefan => correct');
        } else {
          console.warn('value (via item name)  != Stefan => incorrect');
        }

        console.info('Test Item Object vs. Item Name Resilience. Now using Object directly attached in block');

        if (((items.metadata.getMetadata(_itemName(items.getItem('teststate')), namespace)!=null) ? (items.metadata.getMetadata(_itemName(items.getItem('teststate')), namespace).value) : 'undefined') == 'Stefan') {
          console.info('value (via item object)  == Stefan => correct');
        } else {
          console.warn('value (via item object)  != Stefan => incorrect');
        }

        console.info('Test Item Object vs. Item Name Resilience. Now using Object through intransparent var');

        item = items.getItem('teststate');

        if (((items.metadata.getMetadata(_itemName(item), namespace)!=null) ? (items.metadata.getMetadata(_itemName(item), namespace).value) : 'undefined') == 'Stefan') {
          console.info('value (via item object)  == Stefan => correct');
        } else {
          console.warn('value (via item object)  != Stefan => incorrect');
        }


        console.info('Testing writes');

        console.info('Setting Metadata Value');

        itemMedatata = items.metadata.getMetadata(_itemName(item), namespace)

        itemMedatata.value = 'ON'

        items.metadata.replaceMetadata(_itemName(item),namespace,itemMedatata.value,itemMedatata.configuration);

        if (((items.metadata.getMetadata(_itemName('teststate'), namespace)!=null) ? (items.metadata.getMetadata(_itemName('teststate'), namespace).value) : 'undefined') == 'ON') {
          console.info('value (via item name) == ON => correct');
        } else {
          console.warn('value (via item name)  != ON => incorrect');
        }

        itemMedatata = items.metadata.getMetadata(_itemName(item), namespace)

        itemMedatata.value = 'Stefan'

        items.metadata.replaceMetadata(_itemName(item),namespace,itemMedatata.value,itemMedatata.configuration);

        if (((items.metadata.getMetadata(_itemName('teststate'), namespace)!=null) ? (items.metadata.getMetadata(_itemName('teststate'), namespace).value) : 'undefined') == 'Stefan') {
          console.info('value (via item name) == Stefan => correct');
        } else {
          console.warn('value (via item name)  != Stefan => incorrect');
        }


        console.info('Setting Config Values with hierarchies');

        itemMedatata = items.metadata.getMetadata(_itemName('teststate'), namespace)

        if(itemMedatata!==null){
          itemMedatata.configuration.ooone = 'one'
          items.metadata.replaceMetadata(_itemName('teststate'),namespace,itemMedatata.value,itemMedatata.configuration);
        }

        if ((getItemMetaConfigValue(_itemName('teststate'), namespace, 'ooone')) == 'one') {
          console.info('ooone == one => correct');
        } else {
          console.warn('ooone != one => incorrect');
        }

        itemMedatata = items.metadata.getMetadata(_itemName('teststate'), namespace)

        if(itemMedatata!==null){
          itemMedatata.configuration.ooone = '123'
          items.metadata.replaceMetadata(_itemName('teststate'),namespace,itemMedatata.value,itemMedatata.configuration);
        }

        if ((getItemMetaConfigValue(_itemName('teststate'), namespace, 'ooone')) == '123') {
          console.info('ooone == 123 => correct');
        } else {
          console.warn('ooone != 123 => incorrect');
        }

        itemMedatata = items.metadata.getMetadata(_itemName(item), namespace)

        if(itemMedatata!==null){
          itemMedatata.configuration.oone.two = 'fourfivesix'
          items.metadata.replaceMetadata(_itemName(item),namespace,itemMedatata.value,itemMedatata.configuration);
        }

        if ((getItemMetaConfigValue(_itemName('teststate'), namespace, 'oone.two')) == 'fourfivesix') {
          console.info('oone.two == fourfivesix => correct');
        } else {
          console.warn('oone.two != fourfivesix => incorrect');
        }

        itemMedatata = items.metadata.getMetadata(_itemName(item), namespace)

        if(itemMedatata!==null){
          itemMedatata.configuration.oone.two = '456'
          items.metadata.replaceMetadata(_itemName(item),namespace,itemMedatata.value,itemMedatata.configuration);
        }

        if ((getItemMetaConfigValue(_itemName('teststate'), namespace, 'oone.two')) == '456') {
          console.info('oone.two == 456 => correct');
        } else {
          console.warn('oone.two != 456 => incorrect');
        }


        for (i = 1; i <= 3; i++) {
          fieldName = 'field' + String(i);
          if ((getItemMetaConfigValue(_itemName('teststate'), namespace, fieldName)) == i) {
            console.info((['field',i,' == ',i].join('')));
          } else {
            console.info((['field',i,' != ',i].join('')));
            console.info((getItemMetaConfigValue(_itemName('teststate'), namespace, fieldName)));
          }
        }
