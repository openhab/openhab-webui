      script: >
        var runtime = require('@runtime');

        var itemRegistry = runtime.itemRegistry;

        var events = runtime.events;


        var item, memberItem, tag;



        console.error(items.getItem('Lights').state);

        console.info(('name:' + String(items.getItem('Lights').name)));

        console.info(('label:' + String(items.getItem('Lights').label)));

        console.info(('state:' + String(items.getItem('Lights').state)));

        console.info(('category:' + String(items.getItem('Lights').category)));

        console.info(('tags:' + String(items.getItem('Lights').tags)));

        console.info(('type:' + String(items.getItem('Lights').type)));

        console.info(('groups (Lights):' + String(items.getItem('Lights').groupNames)));

        console.info(('groups (Lichter AZ):' + String(items.getItem('LichterAZ').groupNames)));

        console.info(('members of group Lichter AZ: ' + String(items.getItem('LichterAZ').members)));


        console.info(('state:' + String(items.getItem('Lights').state)));

        console.error('For each Member in Group Lights');

        var memberItem_list = items.getItem('Lights').members;

        for (var memberItem_index in memberItem_list) {
          memberItem = memberItem_list[memberItem_index];
          console.info((' member-item:' + String(memberItem)));
          console.info(('    name: ' + String(memberItem.name)));
          console.info(('    state: ' + String(memberItem.state)));
          console.info(('    state by name:' + String(items.getItem(memberItem.name).state)));
        }


        console.info('send ON to Nano Trio');

        items.getItem('Nano_Trio_Power').sendCommand('ON');

        console.error('iterate of tags of item Lights');

        var tag_list = items.getItem('Lights').tags;

        for (var tag_index in tag_list) {
          tag = tag_list[tag_index];
          console.info(tag);
        }


        var item_list = items.getItemsByTag('rs_close');

        for (var item_index in item_list) {
          item = item_list[item_index];
          console.info(item.name);
        }


