    <xsd:attribute name="max" type="xsd:decimal" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Maximum numer of entries to fetch from the router.</xsd:documentation>
        <xsd:documentation xml:lang="de">Maximale Anzahl der vom Router zu ladenden Einträge.</xsd:documentation>
      </xsd:annotation>
