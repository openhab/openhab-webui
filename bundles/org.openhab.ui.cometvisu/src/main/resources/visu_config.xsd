        <xsd:documentation xml:lang="de">Durch Strichpunkt getrennte Liste mit Spaltentypen
          (mögliche Spalten: caller, date, name, nameOrCaller, tam, type)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="TAM" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Icon for telephone answering machine.</xsd:documentation>
        <xsd:documentation xml:lang="de">Icon für Anrufbeantworter.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="TAMColor" type="xsd:string" use="optional" />
    <xsd:attribute name="TAMwait" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Icon for waiting for telephone answering machine.</xsd:documentation>
        <xsd:documentation xml:lang="de">Icon für Warten auf Anrufbeantworter.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="TAMwaitColor" type="xsd:string" use="optional" />
    <xsd:attribute name="TAMplay" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Icon for playing the telephone answering machine.</xsd:documentation>
        <xsd:documentation xml:lang="de">Icon für Anrufbeantworter abspielen.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="TAMplayColor" type="xsd:string" use="optional" />
    <xsd:attribute name="TAMstop" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Icon for stopping the telephone answering machine.</xsd:documentation>
        <xsd:documentation xml:lang="de">Icon für Anrufbeantworter Stopp.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="TAMstopColor" type="xsd:string" use="optional" />
    <xsd:attribute name="typeIncoming" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Icon for incoming call.</xsd:documentation>
        <xsd:documentation xml:lang="de">Icon für eingehenden Anruf.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="typeIncomingColor" type="xsd:string" use="optional" />
    <xsd:attribute name="typeMissed" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Icon for missed call.</xsd:documentation>
        <xsd:documentation xml:lang="de">Icon für verpassten Anruf.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="typeMissedColor" type="xsd:string" use="optional" />
    <xsd:attribute name="typeOutgoing" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Icon for outgoing call.</xsd:documentation>
        <xsd:documentation xml:lang="de">Icon für abgehenden Anruf.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="typeOutgoingColor" type="xsd:string" use="optional" />
    <xsd:attribute name="typeActiveIncoming" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Icon for active incoming call.</xsd:documentation>
        <xsd:documentation xml:lang="de">Icon für aktiven, eingehenden Anruf.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="typeActiveIncomingColor" type="xsd:string" use="optional" />
    <xsd:attribute name="typeRejectedIncoming" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Icon for rejected incoming call.</xsd:documentation>
        <xsd:documentation xml:lang="de">Icon für abgelehnten, eingehenden Anruf.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="typeRejectedIncomingColor" type="xsd:string" use="optional" />
    <xsd:attribute name="typeActiveOutgoing" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Icon for active outgoing call.</xsd:documentation>
        <xsd:documentation xml:lang="de">Icon für aktiven, abgehenden Anruf.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="typeActiveOutgoingColor" type="xsd:string" use="optional" />
    <xsd:attribute name="typeUnknown" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Icon for unknown call type.</xsd:documentation>
        <xsd:documentation xml:lang="de">Icon für unbekannten Anruftyp.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="typeUnknownColor" type="xsd:string" use="optional" />
    <xsd:attribute name="refresh" type="xsd:decimal" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Sets the update rate for the call list in seconds</xsd:documentation>
        <xsd:documentation xml:lang="de">Definiert die Aktualisierungsrate der Anrufliste in Sekunden</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="break" />

  <xsd:complexType name="line">
    <xsd:sequence>
      <xsd:element name="layout" type="layout" minOccurs="0" maxOccurs="1" />  
    </xsd:sequence>   
  </xsd:complexType>

  <xsd:complexType name="toggle">  
    <xsd:sequence>
      <xsd:element name="layout" type="layout" minOccurs="0" maxOccurs="1" />  
      <xsd:element name="label" type="label" minOccurs="0" maxOccurs="1" />       
      <xsd:element name="address" type="address" minOccurs="1"  maxOccurs="unbounded"/>   
    </xsd:sequence>   
    <xsd:attribute ref="styling" use="optional" />  
    <xsd:attribute ref="mapping" />  
    <xsd:attribute ref="align" use="optional" />  
    <xsd:attribute ref="flavour" use="optional" />
    <xsd:attribute ref="class" use="optional" />
    <xsd:attribute ref="format" use="optional" />
  </xsd:complexType>

  <xsd:complexType name="axis">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="label" type="xsd:string" >
          <xsd:annotation>
            <xsd:documentation xml:lang="en">Defines the label of the axis</xsd:documentation>
            <xsd:documentation xml:lang="de">Legt die Achsenbeschriftung fest</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="min" type="xsd:decimal" >
          <xsd:annotation>
            <xsd:documentation xml:lang="en">Defines the minimum of y-axis.</xsd:documentation>
            <xsd:documentation xml:lang="de">Definiert das Minimum der y-Achse.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="max" type="xsd:decimal" >
          <xsd:annotation>
            <xsd:documentation xml:lang="en">Defines the maximum of y-axis.</xsd:documentation>
            <xsd:documentation xml:lang="de">Definiert das Maximum der y-Achse.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="position">
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:enumeration value="left" />
              <xsd:enumeration value="right" />
            </xsd:restriction>
          </xsd:simpleType>         
        </xsd:attribute>
        <xsd:attribute name="unit" type="xsd:string" use="optional" >
          <xsd:annotation>
            <xsd:documentation xml:lang="en">Defines the unit shown behind values in y-axis and tooltip</xsd:documentation>
            <xsd:documentation xml:lang="de">Legt die Einheit fest, die hinter den Zahlen der y-Achsenbeschriftung bzw. im Tooltip angezeigt werden</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="decimals" type="xsd:integer" use="optional" >
          <xsd:annotation>
            <xsd:documentation xml:lang="en">Defines the number of digits in values of y-axis</xsd:documentation>
            <xsd:documentation xml:lang="de">Legt die Anzahl der Nachkommastellen fest, die in der y-Achsenbeschriftung bzw. im Tooltip angezeigt werden</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="rrd">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="yaxis" type="xsd:string" >
          <xsd:annotation>
            <xsd:documentation xml:lang="en">Assigns the rrd-data to a y-axis</xsd:documentation>
            <xsd:documentation xml:lang="de">Weist die Daten einer y-Achse zu</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="color" type="xsd:string" >
          <xsd:annotation>
            <xsd:documentation xml:lang="en">Defines the color of the data-line in the graph</xsd:documentation>
            <xsd:documentation xml:lang="de">Legt die Farbe der Diagrammlinie fest</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="label" type="xsd:string" >
          <xsd:annotation>
            <xsd:documentation xml:lang="en">Defines the label of the data-series in the legend</xsd:documentation>
            <xsd:documentation xml:lang="de">Defininiert den Namen der Datenreihe in der Legende</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="scaling" type="xsd:decimal" />
        <xsd:attribute name="steps" type="xsd:boolean" />
        <xsd:attribute name="fill" type="xsd:boolean" >
          <xsd:annotation>
            <xsd:documentation xml:lang="en">Fill the space below the data-series or bar</xsd:documentation>
            <xsd:documentation xml:lang="de">Füllt den Bereich unterhalb der Datenreihe bzw. Säule</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="style" use="optional" >
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:enumeration value="lines" />
              <xsd:enumeration value="bars" />
              <xsd:enumeration value="points" />
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="barWidth" type="xsd:integer" use="optional" >
          <xsd:annotation>
            <xsd:documentation xml:lang="en">Width of the bars in units of the x axis, contrary to most other measures that are specified in pixels. For instance, for time series the unit is milliseconds so 24 * 60 * 60 * 1000 produces bars with the width of a day.</xsd:documentation>
            <xsd:documentation xml:lang="de">Breite der Balken in Einheiten der X Achse, im Gegensatz zu anderen Werten die in Pixel angegeben werden. Für Zeitverläufe z.B. ist die Einheit Millisekunden, sodass 24 * 60 * 60 * 1000 Balken in der Breite von einem Tag erzeugt.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute ref="align" use="optional" />
        <xsd:attribute name="datasourceIndex" type="xsd:integer" use="optional" default="0" >
          <xsd:annotation>
            <xsd:documentation xml:lang="en">If there are multiple datasources in the RRD file this index defines which one to show. Use "rrdtool info" to find the correct index.</xsd:documentation>
            <xsd:documentation xml:lang="de">Im Falle mehrerer Datenquellen in einer RRD-Datei kann hier der Index der Datenquelle angegeben werden, die angezeigt werden soll. Um den richtigen Index herauszufinden, kann "rrdtool info" verwendet werden.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="consolidationFunction" use="optional" default="AVERAGE" >
          <xsd:annotation>
            <xsd:documentation xml:lang="en">Choose one of the consolidation functions of the RRDTool (see http://rrdtool.org).</xsd:documentation>
            <xsd:documentation xml:lang="de">Auswählen einer der verfügbaren Consolidation Functions des RRDTools (http://rrdtool.org).</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:enumeration value="AVERAGE" />
              <xsd:enumeration value="LAST" />
              <xsd:enumeration value="MIN" />
              <xsd:enumeration value="MAX" />
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="resolution" type="xsd:integer" use="optional">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">The standard resolution for retrieving data from the RRD file can be overriden with a custom value in seconds.</xsd:documentation>
            <xsd:documentation xml:lang="de">Die Standard-Auflösung für die Daten aus der RRD-Datei kann überschrieben werden mit einem eigenen Sekunden-Wert.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="offset" type="xsd:integer" use="optional">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">An offset in seconds to modify the timestamp of the retrieved data from the RRD file.</xsd:documentation>
            <xsd:documentation xml:lang="de">Offset in Sekunden, um die Zeitstempel der Daten aus der RRD-Datei zu modifizieren.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="tagInflux">
    <xsd:attribute name="key" type="xsd:string" />
    <xsd:attribute name="operator">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="=" />
          <xsd:enumeration value="!=" />
          <xsd:enumeration value="&lt;" />
          <xsd:enumeration value="&lt;=" />
          <xsd:enumeration value="&gt;" />
          <xsd:enumeration value="&gt;=" />
          <xsd:enumeration value="=~" />
          <xsd:enumeration value="!~" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="value" type="xsd:string" />
  </xsd:complexType>

  <xsd:complexType name="tagAndOrInflux">
    <xsd:choice maxOccurs="unbounded">
      <xsd:element name="tag" type="tagInflux" maxOccurs="unbounded"/>
      <xsd:element name="and" type="tagAndOrInflux" maxOccurs="unbounded"/>
      <xsd:element name="or" type="tagAndOrInflux" maxOccurs="unbounded"/>
    </xsd:choice>
  </xsd:complexType>

  <xsd:complexType name="influx">
    <xsd:choice minOccurs="0" maxOccurs="1">
      <xsd:element name="tag" type="tagInflux" />
      <xsd:element name="and" type="tagAndOrInflux" />
      <xsd:element name="or" type="tagAndOrInflux" />
    </xsd:choice>
    <xsd:attribute name="measurement" type="xsd:string" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Selection of the database and measurement (example: timeseries_db/KNX_LINE1)</xsd:documentation>
        <xsd:documentation xml:lang="de">Auswahl der Datenbank und des Measurement (Beispiel: timeseries_db/KNX_LINE1)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="yaxis" type="xsd:string" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Assigns the InfluxDB-data to a y-axis</xsd:documentation>
        <xsd:documentation xml:lang="de">Weist die InfluxDB-Daten einer y-Achse zu</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="color" type="xsd:string" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Defines the color of the data-line in the graph</xsd:documentation>
        <xsd:documentation xml:lang="de">Legt die Farbe der Diagrammlinie fest</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="label" type="xsd:string" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Defines the label of the data-series in the legend</xsd:documentation>
        <xsd:documentation xml:lang="de">Defininiert den Namen der Datenreihe in der Legende</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="scaling" type="xsd:decimal" />
    <xsd:attribute name="steps" type="xsd:boolean" />
    <xsd:attribute name="fill" type="xsd:boolean" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Fill the space below the data-series or bar</xsd:documentation>
        <xsd:documentation xml:lang="de">Füllt den Bereich unterhalb der Datenreihe bzw. Säule</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="style" use="optional" >
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="lines" />
          <xsd:enumeration value="bars" />
          <xsd:enumeration value="points" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="barWidth" type="xsd:integer" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Width of the bars in units of the x axis, contrary to most other measures that are specified in pixels. For instance, for time series the unit is milliseconds so 24 * 60 * 60 * 1000 produces bars with the width of a day.</xsd:documentation>
        <xsd:documentation xml:lang="de">Breite der Balken in Einheiten der X Achse, im Gegensatz zu anderen Werten die in Pixel angegeben werden. Für Zeitverläufe z.B. ist die Einheit Millisekunden, sodass 24 * 60 * 60 * 1000 Balken in der Breite von einem Tag erzeugt.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute ref="align" use="optional" />
    <xsd:attribute name="authentication" type="xsd:string" use="optional" default="influx" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">When the InfluxDB requires authentication the credentials in the hidden config with this key will be used.</xsd:documentation>
        <xsd:documentation xml:lang="de">Wenn die InfluxDB Authentifizierung benötigt, dann werden die Anmeldeinformationen aus der Hidden-Konfiguration mit diesem Schlüssel verwendet.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="field" type="xsd:string" use="optional" default="*" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">If there are multiple fields in the InfluxDB series this field defines which one to show. (When not set a default of "*" will be used.)</xsd:documentation>
        <xsd:documentation xml:lang="de">Im Falle mehrerer Felder (Spalten) in einer InfluxDB-Zeitreihe kann hier der Name des Feldes angegeben werden, die angezeigt werden soll. (Wenn nicht explizit angegeben wird "*" verwendet.)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="resolution" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">The temporal resolution for the InfluxDB data can be set to a fixed interval (value in seconds).</xsd:documentation>
        <xsd:documentation xml:lang="de">Die zeitliche Auflösung für die Daten aus der InfluxDB kann auf eine eigenes Intervall (Wert in Sekunden) festgesetzt werden.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="consolidationFunction" use="optional" default="MEDIAN" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Choose one of the available data aggregation functions - required when the "resolution" attribute is set.</xsd:documentation>
        <xsd:documentation xml:lang="de">Auswählen einer der verfügbaren Daten Aggregierungs Funktionen - notwendig, wenn das Attribut "resolution" gesetzt ist.</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="COUNT" />
          <xsd:enumeration value="DERIVATIVE" />
          <xsd:enumeration value="DERIVATIVE_COUNT" />
          <xsd:enumeration value="DERIVATIVE_MEAN" />
          <xsd:enumeration value="DERIVATIVE_MEDIAN" />
          <xsd:enumeration value="DERIVATIVE_MODE" />
          <xsd:enumeration value="DERIVATIVE_SUM" />
          <xsd:enumeration value="DERIVATIVE_FIRST" />
          <xsd:enumeration value="DERIVATIVE_LAST" />
          <xsd:enumeration value="DERIVATIVE_MIN" />
          <xsd:enumeration value="DERIVATIVE_MAX" />
          <xsd:enumeration value="DERIVATIVE_PERCENTILE_1" />
          <xsd:enumeration value="DERIVATIVE_PERCENTILE_5" />
          <xsd:enumeration value="DERIVATIVE_PERCENTILE_10" />
          <xsd:enumeration value="DERIVATIVE_PERCENTILE_20" />
          <xsd:enumeration value="DERIVATIVE_PERCENTILE_25" />
          <xsd:enumeration value="DERIVATIVE_PERCENTILE_75" />
          <xsd:enumeration value="DERIVATIVE_PERCENTILE_80" />
          <xsd:enumeration value="DERIVATIVE_PERCENTILE_90" />
          <xsd:enumeration value="DERIVATIVE_PERCENTILE_95" />
          <xsd:enumeration value="DERIVATIVE_PERCENTILE_99" />
          <xsd:enumeration value="DIFFERENCE" />
          <xsd:enumeration value="DIFFERENCE_COUNT" />
          <xsd:enumeration value="DIFFERENCE_MEAN" />
          <xsd:enumeration value="DIFFERENCE_MEDIAN" />
          <xsd:enumeration value="DIFFERENCE_MODE" />
          <xsd:enumeration value="DIFFERENCE_SUM" />
          <xsd:enumeration value="DIFFERENCE_FIRST" />
          <xsd:enumeration value="DIFFERENCE_LAST" />
          <xsd:enumeration value="DIFFERENCE_MIN" />
          <xsd:enumeration value="DIFFERENCE_MAX" />
          <xsd:enumeration value="DIFFERENCE_PERCENTILE_1" />
          <xsd:enumeration value="DIFFERENCE_PERCENTILE_5" />
          <xsd:enumeration value="DIFFERENCE_PERCENTILE_10" />
          <xsd:enumeration value="DIFFERENCE_PERCENTILE_20" />
          <xsd:enumeration value="DIFFERENCE_PERCENTILE_25" />
          <xsd:enumeration value="DIFFERENCE_PERCENTILE_75" />
          <xsd:enumeration value="DIFFERENCE_PERCENTILE_80" />
          <xsd:enumeration value="DIFFERENCE_PERCENTILE_90" />
          <xsd:enumeration value="DIFFERENCE_PERCENTILE_95" />
          <xsd:enumeration value="DIFFERENCE_PERCENTILE_99" />
          <xsd:enumeration value="ELAPSED" />
          <xsd:enumeration value="INTEGRAL" />
          <xsd:enumeration value="MAX" />
          <xsd:enumeration value="MEAN" />
          <xsd:enumeration value="MEDIAN" />
          <xsd:enumeration value="MIN" />
          <xsd:enumeration value="MODE" />
          <xsd:enumeration value="SPREAD" />
          <xsd:enumeration value="STDDEV" />
          <xsd:enumeration value="SUM" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="fillMissing" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Optional when attribute "resolution" is set: fill the time steps where data is missing by this value or one of "linear", "none", "null" or "previous".</xsd:documentation>
        <xsd:documentation xml:lang="de">Option wenn Attribut "resolution" gesetzt ist: fülle Zeitpunkte für die keine Daten vorhanden sind mit diesem Wert oder einem aus "linear", "none", "null" oder "previous".</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="offset" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">An offset in seconds to modify the displayed timestamps of the retrieved data from the InfluxDB.</xsd:documentation>
        <xsd:documentation xml:lang="de">Offset in Sekunden, um die angezeigten Zeitstempel der Daten aus der InfluxDB zu modifizieren.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="infoaction">
    <xsd:sequence>
      <xsd:element name="layout" type="layout" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="label" type="label" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="widgetinfo" type="widgetinfo" />
      <xsd:element name="widgetaction" type="widgetaction" />
    </xsd:sequence>
  </xsd:complexType>
  
  <xsd:complexType name="widgetinfo">
    <xsd:sequence>
      <xsd:choice minOccurs="1" maxOccurs="1">
        <xsd:group ref="Infoaction_WidgetInfos"/>
      </xsd:choice>     
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="widgetaction">
    <xsd:sequence>
      <xsd:choice minOccurs="1" maxOccurs="1">
        <xsd:group ref="Infoaction_WidgetActions"/>
      </xsd:choice>     
    </xsd:sequence>
  </xsd:complexType>
  
  <xsd:group name="Infoaction_WidgetInfos">
    <xsd:choice>
      <xsd:element name="text" type="text" />
      <xsd:element name="info" type="info" />
    </xsd:choice>
  </xsd:group>
  
  <xsd:group name="Infoaction_WidgetActions">
    <xsd:choice>
      <xsd:element name="switch" type="switch" />
      <xsd:element name="toggle" type="toggle" />
      <xsd:element name="trigger" type="trigger" />
      <xsd:element name="pushbutton" type="pushbutton" />
      <xsd:element name="urltrigger" type="urltrigger" />
      <xsd:element name="multitrigger" type="multitrigger" />
      <xsd:element name="image" type="image" />
      <xsd:element name="imagetrigger" type="imagetrigger" />
      <xsd:element name="pagejump" type="pagejump" />
      <xsd:element name="info" type="info" />
    </xsd:choice>
  </xsd:group>
  
  <xsd:complexType name="mobilemenu">
    <xsd:choice>
      <xsd:element name="switch" type="switch" />
      <xsd:element name="text" type="text" />
      <xsd:element name="toggle" type="toggle" />
      <xsd:element name="trigger" type="trigger" />
      <xsd:element name="pushbutton" type="pushbutton" />
      <xsd:element name="urltrigger" type="urltrigger" />
      <xsd:element name="multitrigger" type="multitrigger" />
      <xsd:element name="image" type="image" />
      <xsd:element name="imagetrigger" type="imagetrigger" />
      <xsd:element name="pagejump" type="pagejump" />
      <xsd:element name="info" type="info" />
    </xsd:choice>
  </xsd:complexType>

  <xsd:complexType name="speech">
    <xsd:sequence>
      <xsd:element name="address" type="address" minOccurs="1"  maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute ref="lang" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">The language that should be used to speak the text.</xsd:documentation>
        <xsd:documentation xml:lang="de">Die Stimme die benutzt werden soll, um den Text zu sprechen.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute ref="mapping" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          Map the bus value to a message for text-to-speech output. You can e.g. map numbers to messages.
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Ordnet den Werten vom Bus Texte zur Sprachausgabe zu. Man kann z.B. Zahlen auf Nachrichten mappen.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="repeat-timeout" type="xsd:nonNegativeInteger" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          Do not repeat saying the same content within the given timeout (in seconds).
          0 means: never repeat the same content.
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Zeitraum in Sekunden innerhalb dessen der selbe Text nicht wiederholt wird.
          Wird der Wert 0 angegeben, wird der Text niemals zweimal hintereinander gesagt.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  
  
  <!-- OWM PlugIn -->
  <xsd:complexType name="openweathermap">
    <xsd:attribute name="lang" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          Language / format of the date
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Sprache / Datum-Formatierung
        </xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="en" />
          <xsd:enumeration value="de" />
          <xsd:enumeration value="fr" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="appid" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          Personal access key as obtained from https://openweathermap.org/
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Persönlicher Zugriffsschlüssel von https://openweathermap.org/
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="description" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          Describing text, displays the place name when left empty. "false" disables the display
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Beschreibungstext, zeigt den Ortsname an, wenn leergelassen. "false" deaktiviert Anzeige
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="detailItems" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          Displays details when set to 1 (default) and hides them when set to 0
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Anzeige von Details wenn Wert 1 ist (Voreinstellung). 0 deaktiviert Anzeige
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="forecast24hItems" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          Number of items to show (0...8)
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Anzahl der Einträge (0...8)
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="forecastDailyItems" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          Number of items to show (0...4)
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Anzahl der Einträge (0...4)
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="showSunrise" type="xsd:boolean" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          Show the sun rise and set information
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Sonnenauf- und untergang anzeigen
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="owID" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          OpenWeatherMap city ID as defined in http://bulk.openweathermap.org/sample/city.list.json.gz
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          OpenWeatherMap City ID aus http://bulk.openweathermap.org/sample/city.list.json.gz
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="q" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          When owID is not used: City name and country code divided by comma, use ISO 3166 country codes
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Wenn owID nicht verwendet wird: Ortsname und ISO 3166 Ländercode, durch Komma getrennt
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="lat" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          When owID or q is not used: latitude of place
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Wenn owID oder q nicht verwendet wird: Geographische Breite des Ortes
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="lon" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          When owID or q is not used: longitue of place
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Wenn owID oder q nicht verwendet wird: Geographische Länge des Ortes
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="refresh" type="xsd:integer" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          Refresh interval of the data in minutes
        </xsd:documentation>
        <xsd:documentation xml:lang="de">
          Aktualisierungsintervall in Minuten
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute ref="class" use="optional" />
  </xsd:complexType>
</xsd:schema>
