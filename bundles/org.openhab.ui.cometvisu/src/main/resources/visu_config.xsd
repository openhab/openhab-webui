        <xsd:documentation xml:lang="de">Definiert die Zeit, bis die festgelegte Seite aufgerufen wird.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute ref="target" use="required" />
    <xsd:attribute name="debug" type="xsd:boolean" use="optional" />
