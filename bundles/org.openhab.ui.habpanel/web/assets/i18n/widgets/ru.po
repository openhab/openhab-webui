msgstr "Виджет позволяет выбрать между несколькими вариантами"

#. Settings dialog title
msgctxt "widget.selection.settings.header"
msgid "Selection Settings"
msgstr "Настройки выбора"

#. Dropdown header
msgctxt "widget.selection.choices_source"
msgid "Choices source"
msgstr "Выбор источника"

#. Dropdown option
msgctxt "widget.selection.choices_source.server"
msgid "Server-provided item options"
msgstr "Опции элемента, предоставленные сервером"

#. Dropdown option
msgctxt "widget.selection.choices_source.csv"
msgid "Comma-separated list"
msgstr "Список, разделенный запятыми"

